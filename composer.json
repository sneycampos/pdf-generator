{
    "require": {
        "gotenberg/gotenberg-php": "^2.12",
        "slim/slim": "4.*",
        "slim/psr7": "^1.7",
        "guzzlehttp/psr7": "^2.7",
        "php-http/curl-client": "^2.3",
        "php-http/message": "^1.16",
        "vlucas/valitron": "^1.4",
        "vlucas/phpdotenv": "^5.6",
        "monolog/monolog": "^3.8"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
